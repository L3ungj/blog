{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://L3ungj.github.io/blog",
  "scripts": {
    "predeploy": "npm run build && echo. > out/.nojekyll",
    "deploy": "gh-pages -d out --dotfiles && dotenv npm run ftp-deploy",
    "ftp-deploy": "ftp-deploy --username %FTP_USERNAME% --password %FTP_PASSWORD% --server %FTP_SERVER% --local-dir ./out/ --server-dir /%DOMAIN_NAME%/htdocs/blog/",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.3",
    "@react-three/fiber": "^9.3.0",
    "@samkirkland/ftp-deploy": "^1.2.5",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.13.0",
    "next": "^15.5.2",
    "ogl": "^1.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-latex-next": "^3.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "reagraph": "^4.30.3",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
